{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": ["elements", "condition"],
	"properties": {
		"elements": {
			"type": "object",
			"patternProperties": {
				"^E\\d+$": {
					"type": "object",
					"required": ["first", "second"],
					"properties": {
						"first": { "type": "number" },
						"second": { "type": "number" }
					},
					"additionalProperties": false
				}
			},
			"additionalProperties": false
		},
		"condition": { "type": "string" }
	},
	"additionalProperties": false
}
